{
  "manifest_version": 2,

  "name": "Naruto Tracker",
  "description": "Tracks next naruto episode.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}